{
  "name": "nest-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "./scripts/build.sh",
    "build:binary": "./scripts/build-binary",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "ts-node-dev --ignore-watch node_modules -r tsconfig-paths/register ./src/main.ts",
    "dev:lint": "nodemon --config ./nodemon-lint.json",
    "deploy": "./scripts/deploy.sh",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "pm2 start --name nest-boilerplate ./dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cli": "ts-node -r tsconfig-paths/register src/console.ts"
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/schedule": "^0.1.1",
    "@nestjs/serve-static": "^1.0.1",
    "@nestjs/typeorm": "^6.2.0",
    "@types/body-parser": "^1.17.1",
    "@types/chalk": "^2.2.0",
    "@types/cheerio": "^0.22.14",
    "@types/form-data": "^2.5.0",
    "@types/fs-extra": "^8.0.1",
    "@types/geoip-lite": "^1.1.29",
    "@types/lodash": "^4.14.149",
    "@types/md5-file": "^4.0.1",
    "@types/mkdirp": "^0.5.2",
    "@types/multer": "^1.3.10",
    "@types/mv": "^2.1.0",
    "@types/node-schedule": "^1.2.4",
    "@types/nodemailer": "^6.2.2",
    "@types/prompts": "^2.0.3",
    "@types/puppeteer": "^2.0.0",
    "@types/qrcode": "^1.3.4",
