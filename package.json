    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.11.0",
    "cls-hooked": "^4.2.2",
    "commander": "^4.0.1",
    "csurf": "^1.10.0",
    "form-data": "^3.0.0",
